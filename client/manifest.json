{
  "manifest_version": 2,

  "name": "Auto STARS Loginer",
  "description": "This extension automatically fills the required forms that required to enter STARS",
  "version": "1.0.1",
  "permissions": ["tabs", "storage", "https://stars.bilkent.edu.tr/accounts/login/*", "https://stars.bilkent.edu.tr/accounts/site/*"],
  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
  "content_scripts": [
    {
      "matches": ["https://stars.bilkent.edu.tr/accounts/login/*"],
      "js": ["complete.js"]
    },
    {
       "matches": ["https://stars.bilkent.edu.tr/accounts/site/*"],
       "js": ["getcode.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}